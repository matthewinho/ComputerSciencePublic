input1 = input("Enter elements of the list separated by space: ")
Binar = input1.split()  

Binar= [int(num) for num in Binar]  

print("List:", Binar)

C = 0
L = 0
minus = -1 
length = len(Binar)
#print(length)

while L < length:
    x = 1*2**L
    C = C  + x
    L = L + 1
    
    list.remove(0)
      
    
   # XD = int(Binar[minus])
    print(C)
